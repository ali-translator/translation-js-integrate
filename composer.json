{
    "name": "ali-translator/translation-js-integrate",
    "description": "Integrate ALI translation to js frontend",
    "keywords": [
        "sli"
    ],
    "homepage": "https://sli.su/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bohdan Rykhal",
            "email": "sli@sli.su",
            "homepage": "https://sli.su/"
        },
        {
            "name": "Slayer911",
            "email": "garolds911@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ali-translator/translation-js-integrate/issues?state=open",
        "source": "https://github.com/ali-translator/translation-js-integrate"
    },
    "require": {
        "php": ">=5.6.0",
        "ali-translator/translation": "dev-master",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "ALI\\TranslationJsIntegrate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ALI\\TranslationJsIntegrate\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "phpunit/phpunit": "^9.0",
        "php": ">=7.3.0"
    }
}
