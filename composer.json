{
    "name": "ali-translator/translator-js-integrate",
    "description": "Integrate ALI translator to frontend js",
    "keywords": [
        "sli", "translator", "js translator"
    ],
    "homepage": "https://sli.su/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Slayer911",
            "email": "garolds911@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ali-translator/translator-js-integrate/issues?state=open",
        "source": "https://github.com/ali-translator/translator-js-integrate"
    },
    "require": {
        "php": ">=7.1",
        "ali-translator/translator": "^0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "ALI\\TranslatorJsIntegrate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ALI\\TranslatorJsIntegrate\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0",
        "phpunit/phpunit": "^9.0",
        "php": ">=7.3.0"
    }
}
